    "workbench.tree.indent": 24,
    "editor.minimap.renderCharacters": false,
    "workbench.colorCustomizations": {
        
        "editorError.foreground":   "#00000000",
        "editorWarning.foreground": "#00000000",
        "editorInfo.foreground":    "#00000000",
        
        "editorGutter.addedBackground": "#2a3926",
        "editorGutter.deletedBackground": "#3b2b2b",
        "editorGutter.modifiedBackground": "#333",
        
        "[Visual Studio Dark]": {
            "activityBar.background": "#202020",
            "activityBar.border": "#303030",
            "editor.background": "#171717",
            
            "editorBracketMatch.border": "#ff000000",
            
            "editor.lineHighlightBorder": "#ff000000",
            "editor.lineHighlightBackground": "#202020",
            
            "sideBar.background": "#202020",
            "sideBar.border": "#303030",
            
            "widget.shadow": "#ffffff00",
            "scrollbar.shadow": "#ffffff00",
            "statusBar.background": "#171717",
            "scrollbarSlider.background": "#ffffff18",
            
            "tab.border": "#303030",
            "tab.inactiveBackground": "#202020",
            "tab.activeBackground": "#171717",
            "terminal.tab.activeBorder": "#303030",
            "terminal.background": "#171717",
            "terminal.border": "#303030",
            
