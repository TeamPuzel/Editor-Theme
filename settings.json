{
    "workbench.editor.untitled.hint": "hidden",
    "terminal.integrated.gpuAcceleration": "off",
    "workbench.iconTheme": "vs-minimal",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "security.workspace.trust.untrustedFiles": "open",
    "editor.fontFamily": "SFMono-Regular",
    "editor.inlineSuggest.enabled": true,
    "telemetry.telemetryLevel": "off",
    "workbench.colorTheme": "Visual Studio Dark",
    "vscode_vibrancy.type": "ultra-dark",
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "files.autoSave": "afterDelay",
    "editor.trimAutoWhitespace": false,
    "editor.smoothScrolling": true,
    "workbench.list.smoothScrolling": true,
    "rust-analyzer.cargo.buildScripts.overrideCommand": null,
    "rust-analyzer.checkOnSave.allTargets": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.guides.indentation": false,
    "editor.lineHeight": 20,
    
    "rust-analyzer.semanticHighlighting.operator.enable": false,
    "rust-analyzer.semanticHighlighting.punctuation.enable": false,
    
    "zenMode.centerLayout": false,
    
    "editor.semanticTokenColorCustomizations": {
        "rules": {
            "keyword": "#fa4673",
            "*.mutable": {
                "underline": false
            },
            "class": "#65d077",
        }
    },
    
    "editor.tokenColorCustomizations": {
        "[Visual Studio Dark]": {
            "keywords": "#fa4673",
            "strings": "#fe9757",
            "comments": "#6f868f",
            "numbers": "#ffbb00",
            "types": "#9bc485",
            "variables": "#d3d1d1",
            "functions": "#a6a6a6"
        }
    },
    
    "workbench.colorCustomizations": {
    "[Visual Studio Dark]": {
        "activityBar.background": "#0a0a0a",
        "editor.background": "#171717",
        "sideBar.background": "#101010",
        "breadcrumb.background": "#1d1d1d",
        "statusBar.background": "#131313"
    }
    },
    "editor.fontSize": 13,
    "zenMode.hideLineNumbers": false,
}

